  title = {A phylogeny of the fish family Sparidae (porgies) inferred from mitochondrial
	sequence data},
  journal = {Molecular Phylogenetics and Evolution},
  year = {2004},
  volume = {32},
  pages = {425-434},
  number = {2},
  abstract = {The porgies (Sparidae) comprise a diverse group of neritic fishes
	with a broad geographic distribution. We used mitochondrial DNA sequences
	from partial 16S ribosomal RNA and cytochrome b genes to reconstruct
	the phylogenetic history of these fishes. Sequences from 38 sparid
	species, 10 species in outgroups closely related to sparids, seven
	basal percoid species, and a non-perciform outgroup species were
	analyzed with parsimony and maximum likelihood. The Sparidae were
	monophyletic with the inclusion of Spicara, which is currently placed
	in the Centracanthidae. The genera Spicara, Pagrus, and Pagellus,
	were not monophyletic indicating a need for revision. Two main sparid
	lineages were recovered in all analyses, but the previously proposed
	six sparid subfamilies (Boopsinae, Denticinae, Diplodinae, Pagellinae,
	Pagrinae, and Sparinae) were not monophyletic. This suggests that
	dentition and feeding modes, upon which these subfamilies are based,
	were independently derived multiple times within sparid fishes. There
	was no evidence from the 16S or combined analyses for a monophyletic
	Sparoidea.},
  doi = {10.1016/j.ympev.2004.01.012},
  issn = {1055-7903},
  type = {Journal Article}
}

@Article{Osborne2004,
  author   = {Osborne, R. F. and Melo, Y. C. and Hofmeyr, M. D.},
  journal  = {Biology of Reproduction},
  title    = {Spawning frequency and batch fecundity of Merluccius capensis off the South African west coast},
  year     = {2004},
  issn     = {0006-3363},
  note     = {The Society for the Study of Reproduction 37th Annual Meeting, August 1-4, 2004, University of British Columbia Vancouver, BC, Canada},
  pages    = {232-232},
  volume   = {SI},
  abstract = {Merluccius capensis (shallow-water Cape hake) are serial spawners,
	their spawning activity peaking during autumn and late spring. The
	spawning dynamics of M. capensis, viz. batch fecundity and spawning
	frequency, were studied using histological analysis of 359 ovaries
	collected on the South African west coast during September 1999.
	Because of sampling problems, it was not possible to determine the
	daily peak of spawning activity, and follow the chronological deterioration
	of the post-ovulatory follicles (POFs). Alternatively, the POFs of
	M. capensis were compared to those of anchovy Engraulis capensis,
	which facilitated the allocation of an age to the deteriorating POFs.
	Spawning fraction, estimated from the incidence of females with Day
	1 POFs, was 17.6%, so, on average, M. capensis females are capable
	of spawning a batch of eggs every seven days. Batch fecundity was
	estimated using the hydrated oocyte method and corrected for atresia.
	The uncorrected batch fecundity ranged from 138 962 (53 cm total
	length) to 899 640 (80 cm total length) hydrated oocytes, with a
	mean of 470 463 ± 175 691 (SD) hydrated oocytes. At a mean estimate
	of hydrated oocyte atresia of 14 ± 10%, the atresia corrected batch
	fecundity ranged from 134 225 to 729 680 hydrated oocytes, with a
	mean of 402 263 ± 150 983 hydrated oocytes. The uncorrected mean
	relative batch fecundity was 223 ± 44 hydrated oocytes per gramme
	(ovary-free fish mass) of female. The atresia corrected mean relative
	batch fecundity was 190 ± 44 hydrated oocytes per gramme of female.
	Batch fecundity was positively correlated to total fish length and
	ovary-free fish mass.},
  type     = {Journal Article},
}

