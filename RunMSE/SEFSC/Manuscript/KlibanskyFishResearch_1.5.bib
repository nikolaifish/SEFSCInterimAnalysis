  volume = {297},
  pages = {217-227},
  number = {3},
  abstract = {To understand the impact of young-of-the-year (YOY) fish on food web
	dynamics and water quality, we stocked larval walleye (9 mm TL) (Stizostedion
	vitreum) in six experimental ponds using two fish densities (10 and
