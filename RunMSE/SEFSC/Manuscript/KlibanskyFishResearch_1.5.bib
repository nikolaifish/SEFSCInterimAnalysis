@ARTICLE{Thorsen2006,
  author = {Thorsen, A. and Marshall, C. T. and Kjesbu, O. S.},
  title = {Comparison of various potential fecundity models for north-east Arctic
	cod Gadus morhua, L. using oocyte diameter as a standardizing factor},
  journal = {Journal of Fish Biology},
  year = {2006},
