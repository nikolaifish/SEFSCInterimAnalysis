  volume = {21},
  pages = {205-272},
  number = {5},
  abstract = {The reproductive biology of yellowfin tuna, Thunnus albacares,was
	investigated by sam- pling 19,990 fish caught by purse-seine vessels
	throughout the eastern Pacific Ocean during October 1987 through
	September 1989. Histological examinations of ovaries from 7,121 females
