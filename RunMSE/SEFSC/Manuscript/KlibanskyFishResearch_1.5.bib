@ARTICLE{LoGalbo2002,
  author = {Lo Galbo, A. M. and Carpenter, K. E. and Reed, D. L.},
  title = {Evolution of trophic types in emperor fishes (Lethrinus, Lethrinidae,
	Percoidei) based on cytochrome b gene sequence variation},
