  note = {Times Cited: 15 Bacheler, Nathan M. Bailey, Kevin M. Ciannelli, Lorenzo
	Bartolino, Valerio Chan, Kung-Sik 15 MAR ECOL PROG SER},
  abstract = {Little is known about the ways in which density-dependent and density-independent
	factors influence the spatial dynamics of fish spawning, despite
