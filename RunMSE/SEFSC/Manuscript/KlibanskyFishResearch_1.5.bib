  type = {Journal Article}
}

@ARTICLE{Alonzo2005,
  author = {Alonzo, S. H. and Mangel, M.},
  title = {Sex-change rules, stock dynamics, and the performance of spawning-per-recruit
	measures in protogynous stocks},
  journal = {Fishery Bulletin},
  year = {2005},
