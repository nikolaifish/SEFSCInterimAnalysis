  title = {Egg size evolution in the tropical American oceans},
  journal = {Integrative and Comparative Biology},
  year = {2003},
  volume = {43},
  pages = {947-947},
  number = {6},
  abstract = {The rise of the Central American Isthmus had strong effects on the
