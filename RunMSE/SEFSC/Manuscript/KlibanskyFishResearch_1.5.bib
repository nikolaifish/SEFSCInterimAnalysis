@Article{Robertson1983,
  author   = {Robertson, D. R.},
  journal  = {Environmental Biology of Fishes},
  title    = {On the spawning behavior and spawning cycles of 8 surgeonfishes (Acanthuridae) from the Indo-Pacific},
  year     = {1983},
