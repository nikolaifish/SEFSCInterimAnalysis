}

@ARTICLE{Campos-Mendoza2004,
  author = {Campos-Mendoza, A. and McAndrew, B. J. and Coward, K. and Bromage,
	N.},
  title = {Reproductive response of Nile tilapia (Oreochromis niloticus) to
	photoperiodic manipulation; effects on spawning periodicity, fecundity
